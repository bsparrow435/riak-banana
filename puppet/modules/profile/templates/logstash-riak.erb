input {
  file { path => ["/tmp/console.log"] }
}
filter {
  grok {
    match => { "message" => "%{LAGER_TIMESTAMP:timestamp} \[%{LOGLEVEL}\] %{GREEDYDATA:erl_pid}@%{ERL_MOD_FUN_LINE:erl_modfun} %{GREEDYDATA:log_message}" }
  }
}
output {
  riak {
    bucket => ["logstash_logs"]
    nodes => {"rihanna01" => "8098"}
  }
}
